import requests
from selenium import webdriver
from selenium.webdriver.common.keys import Keys
import time
# respond = requests.get("https://www.baidu.com")
# print(respond.content.decode())

driver = webdriver.Chrome()
driver.get("http://trains.ctrip.com/TrainBooking/SearchTrain.aspx###")
time.sleep(1)
driver.find_element_by_id("notice01").send_keys("北京")
time.sleep(1)
driver.find_element_by_id("notice08").send_keys("上海")
time.sleep(1)
js = 'document.getElementById("dateObj").removeAttribute("readonly");'
time.sleep(1)
driver.execute_script(js)
time.sleep(1)
driver.find_element_by_id("dateObj").clear()
driver.find_element_by_id("dateObj").send_keys("2018-08-19")
time.sleep(1)
driver.find_element_by_id("searchbtn").send_keys(Keys.ENTER)
